      "backgroundColor": "transparent"
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "@",
      "color": "#00ffff",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "$",
      "color": "#dd771e",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    // {
    //    "tag": "%",
    //    "color": "rgb(0, 4, 255)",
    //    "strikethrough": false,
    //    "backgroundColor": "transparent"
    // },
    {
      "tag": "%",
      "color": "rgb(255, 3, 213)",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.minimap.scale": 3,
  "git.suggestSmartCommit": false,
  "projectManager.git.baseFolders": [
    "Color-App-Frontend",
